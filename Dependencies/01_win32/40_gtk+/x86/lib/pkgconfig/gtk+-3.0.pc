prefix=C:\gtk-build\gtk\Win32\release
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include
targets=win32

gtk_binary_version=3.0.0
gtk_host=x86-windows

Name: GTK+
Description: GTK+ Graphical UI Library
Version: 3.24.12
Requires: gdk-3.0 atk >= 2.15.1  cairo >= 1.14.0 cairo-gobject >= 1.14.0 gdk-pixbuf-2.0 >= 2.30.0 gio-2.0 >= 2.57.2
Requires.private: atk epoxy >= 1.4 fribidi >= 0.19.7 gio-2.0 >= 2.57.2
Libs: -L${libdir} -lgtk-3 
Cflags: -I${includedir}/gtk-3.0 
